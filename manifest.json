{
  "name": "Depfu Freshness score",
  "version": "1.1",
  "description": "Adds a freshness score to each dependencies in Depfu",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
  "page_action": {
    "default_title": ""
  },
  "icons": {
    "16": "images/depfu_score16.png",
    "32": "images/depfu_score32.png",
    "48": "images/depfu_score48.png",
    "128": "images/depfu_score128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://depfu.dc.xing.com/*"],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "optional_permissions": ["<all_urls>"]
}
